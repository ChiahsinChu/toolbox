&FORCE_EVAL
  METHOD QS
  &DFT
    BASIS_SET_FILE_NAME BASIS_MOLOPT
    BASIS_SET_FILE_NAME BASIS_ADMM
    BASIS_SET_FILE_NAME BASIS_ADMM_MOLOPT
    BASIS_SET_FILE_NAME BASIS_MOLOPT-HSE06
    POTENTIAL_FILE_NAME GTH_POTENTIALS
    UKS .TRUE.
    &MGRID
      CUTOFF 400
    &END MGRID
    &QS
      EPS_DEFAULT 1e-13
    &END QS
    &SCF
      SCF_GUESS  RESTART
      EPS_SCF  1e-06
      MAX_SCF  25
      &OUTER_SCF
        EPS_SCF  1e-06
        MAX_SCF  25
      &END OUTER_SCF
      &OT
        MINIMIZER  DIIS
        PRECONDITIONER  FULL_SINGLE_INVERSE
        ENERGY_GAP  0.1
      &END OT
    &END SCF
    &XC
      &XC_FUNCTIONAL PBE
      &END XC_FUNCTIONAL
      &vdW_POTENTIAL
        DISPERSION_FUNCTIONAL PAIR_POTENTIAL
        &PAIR_POTENTIAL
          TYPE DFTD3
          PARAMETER_FILE_NAME dftd3.dat
          REFERENCE_FUNCTIONAL PBE
        &END PAIR_POTENTIAL
      &END vdW_POTENTIAL
    &END XC
    &LOCALIZE
      RESTART .TRUE.
      &PRINT
        &LOC_RESTART
        &END LOC_RESTART
        &WANNIER_CENTERS
          FILENAME =wannier.xyz
        &END WANNIER_CENTERS
        &WANNIER_SPREADS
          FILENAME =wannier_spread.out
        &END WANNIER_SPREADS
      &END PRINT
    &END LOCALIZE
    &PRINT
      &V_HARTREE_CUBE
        ADD_LAST NUMERIC
        STRIDE 8 8 1
      &END V_HARTREE_CUBE
      &E_DENSITY_CUBE
        ADD_LAST NUMERIC
        STRIDE 8 8 1
      &END E_DENSITY_CUBE
      &TOT_DENSITY_CUBE
        ADD_LAST NUMERIC
        STRIDE 1 1 1
      &END TOT_DENSITY_CUBE
      &MO_CUBES
        ADD_LAST NUMERIC
        WRITE_CUBE .FALSE.
      &END MO_CUBES
    &END PRINT
  &END DFT
  &SUBSYS
    &TOPOLOGY
      COORD_FILE_FORMAT XYZ
      COORD_FILE_NAME coord.xyz
    &END TOPOLOGY
    &KIND O
      POTENTIAL GTH-PBE-q6
      BASIS_SET DZVP-MOLOPT-SR-GTH
    &END KIND
    &KIND H
      POTENTIAL GTH-PBE-q1
      BASIS_SET DZVP-MOLOPT-SR-GTH
    &END KIND
    &KIND Pt
      POTENTIAL GTH-PBE-q10
      BASIS_SET DZVP-A5-Q10-323-MOL-T1-DERIVED_SET-1
    &END KIND
    &KIND Ag
      POTENTIAL GTH-PBE-q11
      BASIS_SET DZVP-MOLOPT-SR-GTH
    &END KIND
    &KIND Na
      POTENTIAL GTH-PBE-q9
      BASIS_SET DZVP-MOLOPT-SR-GTH
    &END KIND
    &KIND K
      POTENTIAL GTH-PBE-q9
      BASIS_SET DZVP-MOLOPT-SR-GTH
    &END KIND
    &KIND Li
      POTENTIAL GTH-PBE-q3
      BASIS_SET DZVP-MOLOPT-SR-GTH
    &END KIND
    &KIND C
      POTENTIAL GTH-PBE-q4
      BASIS_SET DZVP-MOLOPT-SR-GTH
    &END KIND
    &KIND N
      POTENTIAL GTH-PBE-q5
      BASIS_SET DZVP-MOLOPT-SR-GTH
    &END KIND
    &KIND Cl
      POTENTIAL GTH-PBE-q7
      BASIS_SET DZVP-MOLOPT-SR-GTH
    &END KIND
    &KIND F
      POTENTIAL GTH-PBE-q7
      BASIS_SET DZVP-MOLOPT-SR-GTH
    &END KIND
    &KIND Mg
      POTENTIAL GTH-PBE-q10
      BASIS_SET DZVP-MOLOPT-SR-GTH
    &END KIND
    &KIND Al
      POTENTIAL GTH-PBE-q3
      BASIS_SET DZVP-MOLOPT-SR-GTH
    &END KIND
    &KIND Au
      BASIS_SET DZVP-MOLOPT-SR-GTH
      POTENTIAL GTH-PBE-q11
    &END KIND
    &CELL
      A 9.0474 0.0000 0.0000
      B 0.0000 9.0474 0.0000
      C 0.0000 0.0000 26.4589
    &END CELL
  &END SUBSYS
  &PRINT
    &FORCES ON
    &END FORCES
  &END PRINT
  STRESS_TENSOR NONE
&END FORCE_EVAL
&GLOBAL
  PROJECT cp2k
&END GLOBAL
